{
	"name": "@foretag/tanstack-db-surrealdb",
	"description": "Add Offline / Local First Caching & Syncing to your SurrealDB app with TanstackDB and Loro (CRDTs)",
	"version": "0.1.10",
	"files": [
		"dist"
	],
	"module": "./dist/index.mjs",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"keywords": [
		"surrealdb",
		"tanstackdb",
		"crdts",
		"sync",
		"local-first"
	],
	"author": {
		"name": "Chiru Boggavarapu",
		"email": "chiru@foretag.co"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ForetagInc/tanstack-db-surrealdb"
	},
	"scripts": {
		"build": "tsup src/index.ts --dts --format esm,cjs"
	},
	"dependencies": {
		"@tanstack/db": "^0.4.20",
		"@tanstack/query-db-collection": "^0.3.0",
		"loro-crdt": "^1.8.9"
	},
	"peerDependencies": {
		"surrealdb": "2.0.0-alpha.13"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.4",
		"@tanstack/svelte-db": "^0.1.42",
		"surrealdb": "2.0.0-alpha.13",
		"tsup": "^8.5.0"
	}
}
