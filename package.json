{
	"name": "@foretag/tanstack-db-surrealdb",
	"description": "Add Offline / Local First Caching & Syncing to your SurrealDB app with TanstackDB and Loro (CRDTs)",
	"version": "0.6.6",
	"files": [
		"dist"
	],
	"module": "./dist/index.mjs",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"keywords": [
		"surrealdb",
		"tanstackdb",
		"crdts",
		"sync",
		"local-first"
	],
	"author": {
		"name": "Chiru Boggavarapu",
		"email": "chiru@foretag.co"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ForetagInc/tanstack-db-surrealdb"
	},
	"scripts": {
		"build": "tsup src/index.ts",
		"test": "bun test",
		"test:integration": "bun test tests/surreal.integration.test.ts"
	},
	"dependencies": {
		"loro-crdt": "^1.10.6"
	},
	"peerDependencies": {
		"@tanstack/db": "^0.5.28",
		"@tanstack/query-core": "^5.0.0",
		"@tanstack/query-db-collection": "^1.0.25",
		"@standard-schema/spec": "^1.1.0",
		"surrealdb": "^2.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.4.4",
		"@standard-schema/spec": "^1.1.0",
		"@tanstack/db": "^0.5.29",
		"@tanstack/query-core": "^5.90.20",
		"@tanstack/query-db-collection": "^1.0.26",
		"@tanstack/react-db": "^0.1.73",
		"@tanstack/react-query": "^5.90.21",
		"@types/bun": "^1.3.9",
		"surrealdb": "^2.0.0",
		"tsup": "^8.5.1"
	}
}
